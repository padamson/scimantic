<?xml version="1.0"?>
<rdf:RDF xmlns="http://scimantic.io/"
     xml:base="http://scimantic.io/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:pav="http://purl.org/pav/"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:linkml="https://w3id.org/linkml/">
    <owl:Ontology rdf:about="http://scimantic.io/">
        <owl:versionIRI rdf:resource="http://scimantic.io/v/0.1.1/ontology.ttl"/>
        <terms:license>https://creativecommons.org/licenses/by/4.0/</terms:license>
        <pav:version>0.1.1</pav:version>
        <rdfs:label>scimantic</rdfs:label>
        <skos:definition>A minimal domain ontology for representing the scientific method as provenance chains.</skos:definition>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://purl.org/dc/terms/license -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/license"/>
    


    <!-- http://purl.org/pav/version -->


    <owl:AnnotationProperty rdf:about="http://purl.org/pav/version"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#definition -->


    <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#definition"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#exactMatch -->


    <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#exactMatch"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#inScheme -->


    <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#inScheme"/>
    


    <!-- https://w3id.org/linkml/permissible_values -->


    <owl:AnnotationProperty rdf:about="https://w3id.org/linkml/permissible_values"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://scimantic.io/contradicts -->


    <owl:ObjectProperty rdf:about="http://scimantic.io/contradicts">
        <rdfs:domain rdf:resource="http://scimantic.io/Entity"/>
        <rdfs:range rdf:resource="http://scimantic.io/Hypothesis"/>
        <rdfs:label>contradicts</rdfs:label>
        <skos:definition>An Entity (Evidence/Result) that provides negative evidence against a Hypothesis.</skos:definition>
        <skos:inScheme rdf:resource="http://scimantic.io/schema"/>
    </owl:ObjectProperty>
    


    <!-- http://scimantic.io/derivationOfUncertainty -->


    <owl:ObjectProperty rdf:about="http://scimantic.io/derivationOfUncertainty">
        <rdfs:range rdf:resource="http://scimantic.io/UncertaintyDerivation"/>
        <rdfs:label>derivationOfUncertainty</rdfs:label>
        <skos:definition>How the uncertainty was derived.</skos:definition>
        <skos:inScheme rdf:resource="http://scimantic.io/schema"/>
    </owl:ObjectProperty>
    


    <!-- http://scimantic.io/hasUncertainty -->


    <owl:ObjectProperty rdf:about="http://scimantic.io/hasUncertainty">
        <rdfs:range rdf:resource="http://scimantic.io/UncertaintyModel"/>
        <rdfs:label>hasUncertainty</rdfs:label>
        <skos:definition>Links an entity to its reified uncertainty model.</skos:definition>
        <skos:inScheme rdf:resource="http://scimantic.io/schema"/>
    </owl:ObjectProperty>
    


    <!-- http://scimantic.io/motivates -->


    <owl:ObjectProperty rdf:about="http://scimantic.io/motivates">
        <rdfs:range rdf:resource="http://scimantic.io/Activity"/>
        <rdfs:label>motivates</rdfs:label>
        <skos:definition>A Question that triggers or motivates an Activity.</skos:definition>
        <skos:inScheme rdf:resource="http://scimantic.io/schema"/>
    </owl:ObjectProperty>
    


    <!-- http://scimantic.io/natureOfUncertainty -->


    <owl:ObjectProperty rdf:about="http://scimantic.io/natureOfUncertainty">
        <rdfs:range rdf:resource="http://scimantic.io/UncertaintyNature"/>
        <rdfs:label>natureOfUncertainty</rdfs:label>
        <skos:definition>The nature of the uncertainty (Aleatory or Epistemic).</skos:definition>
        <skos:inScheme rdf:resource="http://scimantic.io/schema"/>
    </owl:ObjectProperty>
    


    <!-- http://scimantic.io/parameter -->


    <owl:ObjectProperty rdf:about="http://scimantic.io/parameter">
        <rdfs:range rdf:resource="http://scimantic.io/Parameter"/>
        <rdfs:label>parameter</rdfs:label>
        <skos:definition>Structured configuration parameter.</skos:definition>
        <skos:inScheme rdf:resource="http://scimantic.io/schema"/>
    </owl:ObjectProperty>
    


    <!-- http://scimantic.io/refines -->


    <owl:ObjectProperty rdf:about="http://scimantic.io/refines">
        <rdfs:range rdf:resource="http://scimantic.io/Hypothesis"/>
        <rdfs:label>refines</rdfs:label>
        <skos:definition>A Result that suggests a specific modification to a Hypothesis.</skos:definition>
        <skos:inScheme rdf:resource="http://scimantic.io/schema"/>
    </owl:ObjectProperty>
    


    <!-- http://scimantic.io/supports -->


    <owl:ObjectProperty rdf:about="http://scimantic.io/supports">
        <rdfs:domain rdf:resource="http://scimantic.io/Entity"/>
        <rdfs:range rdf:resource="http://scimantic.io/Hypothesis"/>
        <rdfs:label>supports</rdfs:label>
        <skos:definition>An Entity (Evidence/Result) that provides positive evidence for a Hypothesis.</skos:definition>
        <skos:inScheme rdf:resource="http://scimantic.io/schema"/>
    </owl:ObjectProperty>
    


    <!-- http://scimantic.io/used -->


    <owl:ObjectProperty rdf:about="http://scimantic.io/used">
        <rdfs:range rdf:resource="http://scimantic.io/Entity"/>
        <rdfs:label>used</rdfs:label>
        <skos:definition>Entity used by an Activity.</skos:definition>
        <skos:inScheme rdf:resource="http://scimantic.io/schema"/>
    </owl:ObjectProperty>
    


    <!-- http://scimantic.io/wasAssociatedWith -->


    <owl:ObjectProperty rdf:about="http://scimantic.io/wasAssociatedWith">
        <rdfs:range rdf:resource="http://scimantic.io/Agent"/>
        <rdfs:label>wasAssociatedWith</rdfs:label>
        <skos:definition>The agent associated with an activity.</skos:definition>
        <skos:inScheme rdf:resource="http://scimantic.io/schema"/>
    </owl:ObjectProperty>
    


    <!-- http://scimantic.io/wasAttributedTo -->


    <owl:ObjectProperty rdf:about="http://scimantic.io/wasAttributedTo">
        <rdfs:range rdf:resource="http://scimantic.io/Agent"/>
        <rdfs:label>wasAttributedTo</rdfs:label>
        <skos:definition>The agent who attributed this entity (Agent).</skos:definition>
        <skos:inScheme rdf:resource="http://scimantic.io/schema"/>
    </owl:ObjectProperty>
    


    <!-- http://scimantic.io/wasDerivedFrom -->


    <owl:ObjectProperty rdf:about="http://scimantic.io/wasDerivedFrom">
        <rdfs:range rdf:resource="http://scimantic.io/Entity"/>
        <rdfs:label>wasDerivedFrom</rdfs:label>
        <skos:definition>Entity derived from another Entity.</skos:definition>
        <skos:inScheme rdf:resource="http://scimantic.io/schema"/>
    </owl:ObjectProperty>
    


    <!-- http://scimantic.io/wasGeneratedBy -->


    <owl:ObjectProperty rdf:about="http://scimantic.io/wasGeneratedBy">
        <rdfs:range rdf:resource="http://scimantic.io/Activity"/>
        <rdfs:label>wasGeneratedBy</rdfs:label>
        <skos:definition>The activity that generated this entity.</skos:definition>
        <skos:inScheme rdf:resource="http://scimantic.io/schema"/>
    </owl:ObjectProperty>
    


    <!-- http://scimantic.io/wasInformedBy -->


    <owl:ObjectProperty rdf:about="http://scimantic.io/wasInformedBy">
        <rdfs:range rdf:resource="http://scimantic.io/Activity"/>
        <rdfs:label>wasInformedBy</rdfs:label>
        <skos:definition>Activity that informed this Activity.</skos:definition>
        <skos:inScheme rdf:resource="http://scimantic.io/schema"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://scimantic.io/accessLevel -->


    <owl:DatatypeProperty rdf:about="http://scimantic.io/accessLevel">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>accessLevel</rdfs:label>
        <skos:definition>Publishing scope (e.g., local, institutional, public).</skos:definition>
        <skos:inScheme rdf:resource="http://scimantic.io/schema"/>
    </owl:DatatypeProperty>
    


    <!-- http://scimantic.io/citation -->


    <owl:DatatypeProperty rdf:about="http://scimantic.io/citation">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>citation</rdfs:label>
        <skos:definition>Bibliographic reference.</skos:definition>
        <skos:inScheme rdf:resource="http://scimantic.io/schema"/>
    </owl:DatatypeProperty>
    


    <!-- http://scimantic.io/content -->


    <owl:DatatypeProperty rdf:about="http://scimantic.io/content">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>content</rdfs:label>
        <skos:definition>The actual text content of the evidence.</skos:definition>
        <skos:inScheme rdf:resource="http://scimantic.io/schema"/>
    </owl:DatatypeProperty>
    


    <!-- http://scimantic.io/generatedAtTime -->


    <owl:DatatypeProperty rdf:about="http://scimantic.io/generatedAtTime">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:label>generatedAtTime</rdfs:label>
        <skos:definition>Time of generation.</skos:definition>
        <skos:inScheme rdf:resource="http://scimantic.io/schema"/>
    </owl:DatatypeProperty>
    


    <!-- http://scimantic.io/id -->


    <owl:DatatypeProperty rdf:about="http://scimantic.io/id">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>id</rdfs:label>
        <skos:definition>Unique identifier (URI) for this node.</skos:definition>
        <skos:inScheme rdf:resource="http://scimantic.io/schema"/>
    </owl:DatatypeProperty>
    


    <!-- http://scimantic.io/label -->


    <owl:DatatypeProperty rdf:about="http://scimantic.io/label">
        <rdfs:label>label</rdfs:label>
        <skos:definition>The human-readable label or text.</skos:definition>
        <skos:inScheme rdf:resource="http://scimantic.io/schema"/>
    </owl:DatatypeProperty>
    


    <!-- http://scimantic.io/method -->


    <owl:DatatypeProperty rdf:about="http://scimantic.io/method">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>method</rdfs:label>
        <skos:definition>Name or description of the method.</skos:definition>
        <skos:inScheme rdf:resource="http://scimantic.io/schema"/>
    </owl:DatatypeProperty>
    


    <!-- http://scimantic.io/publishable -->


    <owl:DatatypeProperty rdf:about="http://scimantic.io/publishable">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:label>publishable</rdfs:label>
        <skos:definition>Flag indicating if this is ready for public promotion.</skos:definition>
        <skos:inScheme rdf:resource="http://scimantic.io/schema"/>
    </owl:DatatypeProperty>
    


    <!-- http://scimantic.io/source -->


    <owl:DatatypeProperty rdf:about="http://scimantic.io/source">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>source</rdfs:label>
        <skos:definition>Source URL or identifier.</skos:definition>
        <skos:inScheme rdf:resource="http://scimantic.io/schema"/>
    </owl:DatatypeProperty>
    


    <!-- http://scimantic.io/unit -->


    <owl:DatatypeProperty rdf:about="http://scimantic.io/unit">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>unit</rdfs:label>
        <skos:definition>Unit of measurement.</skos:definition>
        <skos:inScheme rdf:resource="http://scimantic.io/schema"/>
    </owl:DatatypeProperty>
    


    <!-- http://scimantic.io/value -->


    <owl:DatatypeProperty rdf:about="http://scimantic.io/value">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>value</rdfs:label>
        <skos:definition>The numeric or categorical result.</skos:definition>
        <skos:inScheme rdf:resource="http://scimantic.io/schema"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://scimantic.io/Activity -->


    <owl:Class rdf:about="http://scimantic.io/Activity">
        <rdfs:label>Activity</rdfs:label>
        <skos:definition>A provenance activity.</skos:definition>
        <skos:exactMatch rdf:resource="http://www.w3.org/ns/prov#Activity"/>
        <skos:inScheme rdf:resource="http://scimantic.io/schema"/>
    </owl:Class>
    


    <!-- http://scimantic.io/Agent -->


    <owl:Class rdf:about="http://scimantic.io/Agent">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://scimantic.io/id"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://scimantic.io/id"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://scimantic.io/id"/>
                <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label>Agent</rdfs:label>
        <skos:definition>A provenance agent.</skos:definition>
        <skos:exactMatch rdf:resource="http://www.w3.org/ns/prov#Agent"/>
        <skos:inScheme rdf:resource="http://scimantic.io/schema"/>
    </owl:Class>
    


    <!-- http://scimantic.io/Aleatory -->


    <owl:Class rdf:about="http://scimantic.io/Aleatory">
        <rdfs:subClassOf rdf:resource="http://scimantic.io/UncertaintyModel"/>
        <rdfs:label>Aleatory</rdfs:label>
        <skos:definition>Aleatory uncertainty entities must have nature Aleatory.</skos:definition>
        <skos:exactMatch rdf:resource="https://raw.githubusercontent.com/adelphi23/urref/469137/URREF.ttl#Aleatory"/>
        <skos:inScheme rdf:resource="http://scimantic.io/schema"/>
    </owl:Class>
    


    <!-- http://scimantic.io/Ambiguity -->


    <owl:Class rdf:about="http://scimantic.io/Ambiguity">
        <rdfs:subClassOf rdf:resource="http://scimantic.io/UncertaintyModel"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://scimantic.io/natureOfUncertainty"/>
                <owl:allValuesFrom rdf:resource="http://scimantic.io/UncertaintyNature"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://scimantic.io/natureOfUncertainty"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">0</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://scimantic.io/natureOfUncertainty"/>
                <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label>Ambiguity</rdfs:label>
        <skos:definition>Ambiguity is inherently Epistemic uncertainty.</skos:definition>
        <skos:exactMatch rdf:resource="https://raw.githubusercontent.com/adelphi23/urref/469137/URREF.ttl#Ambiguity"/>
        <skos:inScheme rdf:resource="http://scimantic.io/schema"/>
    </owl:Class>
    


    <!-- http://scimantic.io/Analysis -->


    <owl:Class rdf:about="http://scimantic.io/Analysis">
        <rdfs:subClassOf rdf:resource="http://scimantic.io/Activity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://scimantic.io/used"/>
                <owl:allValuesFrom rdf:resource="http://scimantic.io/Dataset"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://scimantic.io/wasInformedBy"/>
                <owl:allValuesFrom rdf:resource="http://scimantic.io/Experimentation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://scimantic.io/used"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">0</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://scimantic.io/wasInformedBy"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">0</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://scimantic.io/used"/>
                <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://scimantic.io/wasInformedBy"/>
                <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label>Analysis</rdfs:label>
        <skos:definition>The activity of processing a Dataset to produce a Result.</skos:definition>
        <skos:exactMatch rdf:resource="http://scimantic.io/Analysis"/>
        <skos:inScheme rdf:resource="http://scimantic.io/schema"/>
    </owl:Class>
    


    <!-- http://scimantic.io/Dataset -->


    <owl:Class rdf:about="http://scimantic.io/Dataset">
        <rdfs:subClassOf rdf:resource="http://scimantic.io/Entity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://scimantic.io/hasUncertainty"/>
                <owl:allValuesFrom rdf:resource="http://scimantic.io/UncertaintyModel"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://scimantic.io/wasDerivedFrom"/>
                <owl:allValuesFrom rdf:resource="http://scimantic.io/ExperimentalMethod"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://scimantic.io/wasGeneratedBy"/>
                <owl:allValuesFrom rdf:resource="http://scimantic.io/Experimentation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://scimantic.io/hasUncertainty"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">0</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://scimantic.io/wasDerivedFrom"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">0</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://scimantic.io/wasGeneratedBy"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">0</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://scimantic.io/hasUncertainty"/>
                <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://scimantic.io/wasDerivedFrom"/>
                <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://scimantic.io/wasGeneratedBy"/>
                <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://scimantic.io/label"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://scimantic.io/label"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://scimantic.io/label"/>
                <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label>Dataset</rdfs:label>
        <skos:definition>Raw data, observations, or measurements produced by experimentation.</skos:definition>
        <skos:exactMatch rdf:resource="http://scimantic.io/Dataset"/>
        <skos:inScheme rdf:resource="http://scimantic.io/schema"/>
    </owl:Class>
    


    <!-- http://scimantic.io/DesignOfExperiment -->


    <owl:Class rdf:about="http://scimantic.io/DesignOfExperiment">
        <rdfs:subClassOf rdf:resource="http://scimantic.io/Activity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://scimantic.io/used"/>
                <owl:allValuesFrom rdf:resource="http://scimantic.io/Hypothesis"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://scimantic.io/wasInformedBy"/>
                <owl:allValuesFrom rdf:resource="http://scimantic.io/HypothesisFormation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
